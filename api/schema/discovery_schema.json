{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
      "data",
      "included"
    ],
    "properties": {
      "data": {
        "$id": "#/properties/data",
        "type": "array",
        "title": "The Data Schema",
        "items": {
          "$id": "#/properties/data/items",
          "type": "object",
          "title": "The Items Schema",
          "required": [
            "id",
            "type",
            "context",
            "domain",
            "attributes",
            "relationships"
          ],
          "properties": {
            "id": {
              "$id": "#/properties/data/items/properties/id",
              "type": "string",
              "title": "The Id Schema",
              "default": "",
              "examples": [
                "83"
              ],
              "pattern": "^(.*)$"
            },
            "type": {
              "$id": "#/properties/data/items/properties/type",
              "type": "string",
              "title": "The Type Schema",
              "default": "",
              "examples": [
                "featured_sections"
              ],
              "pattern": "^(.*)$"
            },
            "context": {
              "$id": "#/properties/data/items/properties/context",
              "type": "string",
              "title": "The Context Schema",
              "default": "",
              "examples": [
                "Discovery"
              ],
              "pattern": "^(.*)$"
            },
            "domain": {
              "$id": "#/properties/data/items/properties/domain",
              "type": "string",
              "title": "The Domain Schema",
              "default": "",
              "examples": [
                "Discovery"
              ],
              "pattern": "^(.*)$"
            },
            "attributes": {
              "$id": "#/properties/data/items/properties/attributes",
              "type": "object",
              "title": "The Attributes Schema",
              "required": [
                "name",
                "layout",
                "section_settings",
                "description",
                "created_by",
                "last_update_by"
              ],
              "properties": {
                "name": {
                  "$id": "#/properties/data/items/properties/attributes/properties/name",
                  "type": "string",
                  "title": "The Name Schema",
                  "default": "",
                  "examples": [
                    "Trending Nows"
                  ],
                  "pattern": "^(.*)$"
                },
                "layout": {
                  "$id": "#/properties/data/items/properties/attributes/properties/layout",
                  "type": "string",
                  "title": "The Layout Schema",
                  "default": "",
                  "examples": [
                    "vertical"
                  ],
                  "pattern": "^(.*)$"
                },
                "section_settings": {
                  "$id": "#/properties/data/items/properties/attributes/properties/section_settings",
                  "type": "object",
                  "title": "The Section_settings Schema",
                  "required": [
                    "column",
                    "show_section_title",
                    "show_collection_title",
                    "maximum_featureds"
                  ],
                  "properties": {
                    "column": {
                      "$id": "#/properties/data/items/properties/attributes/properties/section_settings/properties/column",
                      "type": "integer",
                      "title": "The Column Schema",
                      "default": 0,
                      "examples": [
                        1
                      ]
                    },
                    "show_section_title": {
                      "$id": "#/properties/data/items/properties/attributes/properties/section_settings/properties/show_section_title",
                      "type": "boolean",
                      "title": "The Show_section_title Schema",
                      "default": false,
                      "examples": [
                        true
                      ]
                    },
                    "show_collection_title": {
                      "$id": "#/properties/data/items/properties/attributes/properties/section_settings/properties/show_collection_title",
                      "type": "boolean",
                      "title": "The Show_collection_title Schema",
                      "default": false,
                      "examples": [
                        false
                      ]
                    },
                    "maximum_featureds": {
                      "$id": "#/properties/data/items/properties/attributes/properties/section_settings/properties/maximum_featureds",
                      "type": "integer",
                      "title": "The Maximum_featureds Schema",
                      "default": 0,
                      "examples": [
                        0
                      ]
                    }
                  }
                },
                "description": {
                  "$id": "#/properties/data/items/properties/attributes/properties/description",
                  "type": "string",
                  "title": "The Description Schema",
                  "default": "",
                  "examples": [
                    "Trending Nowsss"
                  ],
                  "pattern": "^(.*)$"
                },
                "created_by": {
                  "$id": "#/properties/data/items/properties/attributes/properties/created_by",
                  "type": "string",
                  "title": "The Created_by Schema",
                  "default": "",
                  "examples": [
                    "admin@styletheory.co"
                  ],
                  "pattern": "^(.*)$"
                },
                "last_update_by": {
                  "$id": "#/properties/data/items/properties/attributes/properties/last_update_by",
                  "type": "string",
                  "title": "The Last_update_by Schema",
                  "default": "",
                  "examples": [
                    "admin@styletheory.co"
                  ],
                  "pattern": "^(.*)$"
                }
              }
            },
            "relationships": {
              "$id": "#/properties/data/items/properties/relationships",
              "type": "object",
              "title": "The Relationships Schema",
              "required": [
                "collections"
              ],
              "properties": {
                "collections": {
                  "$id": "#/properties/data/items/properties/relationships/properties/collections",
                  "type": "object",
                  "title": "The Collections Schema",
                  "required": [
                    "data",
                    "included"
                  ],
                  "properties": {
                    "data": {
                      "$id": "#/properties/data/items/properties/relationships/properties/collections/properties/data",
                      "type": "array",
                      "title": "The Data Schema",
                      "items": {
                        "$id": "#/properties/data/items/properties/relationships/properties/collections/properties/data/items",
                        "type": "object",
                        "title": "The Items Schema",
                        "required": [
                          "id",
                          "type",
                          "context",
                          "domain"
                        ],
                        "properties": {
                          "id": {
                            "$id": "#/properties/data/items/properties/relationships/properties/collections/properties/data/items/properties/id",
                            "type": "string",
                            "title": "The Id Schema",
                            "default": "",
                            "examples": [
                              "337"
                            ],
                            "pattern": "^(.*)$"
                          },
                          "type": {
                            "$id": "#/properties/data/items/properties/relationships/properties/collections/properties/data/items/properties/type",
                            "type": "string",
                            "title": "The Type Schema",
                            "default": "",
                            "examples": [
                              "featured_collections"
                            ],
                            "pattern": "^(.*)$"
                          },
                          "context": {
                            "$id": "#/properties/data/items/properties/relationships/properties/collections/properties/data/items/properties/context",
                            "type": "string",
                            "title": "The Context Schema",
                            "default": "",
                            "examples": [
                              "Discovery"
                            ],
                            "pattern": "^(.*)$"
                          },
                          "domain": {
                            "$id": "#/properties/data/items/properties/relationships/properties/collections/properties/data/items/properties/domain",
                            "type": "string",
                            "title": "The Domain Schema",
                            "default": "",
                            "examples": [
                              "Discovery"
                            ],
                            "pattern": "^(.*)$"
                          }
                        }
                      }
                    },
                    "included": {
                      "$id": "#/properties/data/items/properties/relationships/properties/collections/properties/included",
                      "type": "array",
                      "title": "The Included Schema"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "included": {
        "$id": "#/properties/included",
        "type": "array",
        "title": "The Included Schema",
        "items": {
          "$id": "#/properties/included/items",
          "type": "object",
          "title": "The Items Schema",
          "required": [
            "id",
            "type",
            "context",
            "domain",
            "attributes"
          ],
          "properties": {
            "id": {
              "$id": "#/properties/included/items/properties/id",
              "type": "string",
              "title": "The Id Schema",
              "default": "",
              "examples": [
                "337"
              ],
              "pattern": "^(.*)$"
            },
            "type": {
              "$id": "#/properties/included/items/properties/type",
              "type": "string",
              "title": "The Type Schema",
              "default": "",
              "examples": [
                "featured_collections"
              ],
              "pattern": "^(.*)$"
            },
            "context": {
              "$id": "#/properties/included/items/properties/context",
              "type": "string",
              "title": "The Context Schema",
              "default": "",
              "examples": [
                "Discovery"
              ],
              "pattern": "^(.*)$"
            },
            "domain": {
              "$id": "#/properties/included/items/properties/domain",
              "type": "string",
              "title": "The Domain Schema",
              "default": "",
              "examples": [
                "Discovery"
              ],
              "pattern": "^(.*)$"
            },
            "attributes": {
              "$id": "#/properties/included/items/properties/attributes",
              "type": "object",
              "title": "The Attributes Schema",
              "required": [
                "name",
                "methods",
                "method_type",
                "banner",
                "dimension",
                "randomize",
                "created_at",
                "created_by",
                "updated_at",
                "last_update_by",
                "collection_settings",
                "notes"
              ],
              "properties": {
                "name": {
                  "$id": "#/properties/included/items/properties/attributes/properties/name",
                  "type": "string",
                  "title": "The Name Schema",
                  "default": "",
                  "examples": [
                    "Lunar Year"
                  ],
                  "pattern": "^(.*)$"
                },
                "methods": {
                  "$id": "#/properties/included/items/properties/attributes/properties/methods",
                  "type": "string",
                  "title": "The Methods Schema",
                  "default": "",
                  "examples": [
                    "products"
                  ],
                  "pattern": "^(.*)$"
                },
                "method_type": {
                  "$id": "#/properties/included/items/properties/attributes/properties/method_type",
                  "type": "string",
                  "title": "The Method_type Schema",
                  "default": "",
                  "examples": [
                    "query"
                  ],
                  "pattern": "^(.*)$"
                },
                "banner": {
                  "$id": "#/properties/included/items/properties/attributes/properties/banner",
                  "type": "string",
                  "title": "The Banner Schema",
                  "default": "",
                  "examples": [
                    "https://styletheory-clothings.s3.ap-southeast-1.amazonaws.com/banners/New+featured/ID/Standard/On+Demand/CNY/Chinese+New+Year.jpg"
                  ],
                  "pattern": "^(.*)$"
                },
                "dimension": {
                  "$id": "#/properties/included/items/properties/attributes/properties/dimension",
                  "type": "object",
                  "title": "The Dimension Schema",
                  "required": [
                    "width",
                    "height"
                  ],
                  "properties": {
                    "width": {
                      "$id": "#/properties/included/items/properties/attributes/properties/dimension/properties/width",
                      "type": "integer",
                      "title": "The Width Schema",
                      "default": 0,
                      "examples": [
                        654
                      ]
                    },
                    "height": {
                      "$id": "#/properties/included/items/properties/attributes/properties/dimension/properties/height",
                      "type": "integer",
                      "title": "The Height Schema",
                      "default": 0,
                      "examples": [
                        720
                      ]
                    }
                  }
                },
                "randomize": {
                  "$id": "#/properties/included/items/properties/attributes/properties/randomize",
                  "type": "boolean",
                  "title": "The Randomize Schema",
                  "default": false,
                  "examples": [
                    false
                  ]
                },
                "created_at": {
                  "$id": "#/properties/included/items/properties/attributes/properties/created_at",
                  "type": "string",
                  "title": "The Created_at Schema",
                  "default": "",
                  "examples": [
                    "2020-01-17T08:15:01.999Z"
                  ],
                  "pattern": "^(.*)$"
                },
                "created_by": {
                  "$id": "#/properties/included/items/properties/attributes/properties/created_by",
                  "type": "string",
                  "title": "The Created_by Schema",
                  "default": "",
                  "examples": [
                    "ricky@styletheory.co"
                  ],
                  "pattern": "^(.*)$"
                },
                "updated_at": {
                  "$id": "#/properties/included/items/properties/attributes/properties/updated_at",
                  "type": "string",
                  "title": "The Updated_at Schema",
                  "default": "",
                  "examples": [
                    "2020-01-20T05:26:55.421Z"
                  ],
                  "pattern": "^(.*)$"
                },
                "last_update_by": {
                  "$id": "#/properties/included/items/properties/attributes/properties/last_update_by",
                  "type": "string",
                  "title": "The Last_update_by Schema",
                  "default": "",
                  "examples": [
                    "ricky@styletheory.co"
                  ],
                  "pattern": "^(.*)$"
                },
                "collection_settings": {
                  "$id": "#/properties/included/items/properties/attributes/properties/collection_settings",
                  "type": "object",
                  "title": "The Collection_settings Schema"
                },
                "notes": {
                  "$id": "#/properties/included/items/properties/attributes/properties/notes",
                  "type": "string",
                  "title": "The Notes Schema",
                  "default": "",
                  "examples": [
                    "sg"
                  ],
                  "pattern": "^(.*)$"
                }
              }
            }
          }
        }
      }
    }
  }